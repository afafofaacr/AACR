        <externalId>false</externalId>
        <formula>DATETIMEVALUE(TEXT(YEAR( DATEVALUE(Event__r.Start_Date__c) )) + &quot;-&quot; + TEXT(MONTH(DATEVALUE(Event__r.Start_Date__c))) + &quot;-&quot; + TEXT(DAY(DATEVALUE(Event__r.Start_Date__c)))+&quot; &quot;+TEXT(VALUE(LEFT(TEXT(Start_Time__c), 2)))+  MID(TEXT(Start_Time__c),3,3)+&quot;:00&quot;)</formula>
