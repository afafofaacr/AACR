        <formula>LEFT(IF( INCLUDES(Relationship_Type__c, &apos;Employment&apos;), &apos;E,&apos;, &apos;&apos;)
&amp; IF( INCLUDES(Relationship_Type__c, &apos;Fiduciary Officer&apos;), &apos;FO,&apos;, &apos;&apos;)
