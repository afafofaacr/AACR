&amp; IF( INCLUDES(Relationship_Type__c, &apos;Travel&apos;), &apos;T,&apos;, &apos;&apos;)
&amp; IF( INCLUDES(Relationship_Type__c, &apos;Gift&apos;), &apos;G,&apos;, &apos;&apos;)
&amp; IF( INCLUDES(Relationship_Type__c, &apos;Patent&apos;), &apos;P,&apos;, &apos;&apos;)
&amp; IF( INCLUDES(Relationship_Type__c, &apos;Trademark&apos;), &apos;TM,&apos;, &apos;&apos;)
&amp; IF( INCLUDES(Relationship_Type__c, &apos;Copyright&apos;), &apos;C,&apos;, &apos;&apos;)
&amp; IF( INCLUDES(Relationship_Type__c, &apos;Other Intellectual Property&apos;), &apos;OIP,&apos;, &apos;&apos;)
&amp; IF( INCLUDES(Relationship_Type__c, &apos;Other&apos;), &apos;O,&apos;, &apos;&apos;),

LEN(IF( INCLUDES(Relationship_Type__c, &apos;Employment&apos;), &apos;E,&apos;, &apos;&apos;)
&amp; IF( INCLUDES(Relationship_Type__c, &apos;Fiduciary Officer&apos;), &apos;FO,&apos;, &apos;&apos;)
&amp; IF( INCLUDES(Relationship_Type__c, &apos;Independent Contractor&apos;), &apos;IC,&apos;, &apos;&apos;)
