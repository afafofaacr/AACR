var myquery = &quot;SELECT Id, {!$Setup.OrderApi__App_Config__c.OrderApi__Namespace_Prefix__c}Is_Active__c FROM {!$Setup.OrderApi__App_Config__c.OrderApi__Namespace_Prefix__c}Subscription_Plan__c WHERE Id = &apos;{!OrderApi__Subscription__c.OrderApi__Subscription_PlanId__c}&apos;&quot;;
