newLine.{!$Setup.OrderApi__App_Config__c.OrderApi__Namespace_Prefix__c}Enable_Auto_Renew__c = &quot;{!OrderApi__Subscription__c.OrderApi__Enable_Auto_Renew__c}&quot;;
result2 = sforce.connection.create([newLine]);

