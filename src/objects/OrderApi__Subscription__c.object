records = result.getArray(&quot;records&quot;);
if (records[0] &amp;&amp; records[0].{!$Setup.OrderApi__App_Config__c.OrderApi__Namespace_Prefix__c}Is_Active__c === &apos;false&apos;) {
    alert(&apos;Cannot renew with inactive Subscription Plan&apos;);
}
else {
var newOrder= new sforce.SObject(&quot;{!$Setup.OrderApi__App_Config__c.OrderApi__Namespace_Prefix__c}Sales_Order__c&quot;);
