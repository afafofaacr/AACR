    <fields>
        <fullName>MembershipCodeFormula__c</fullName>
        <description>Formula that will generate the Membership Code, upon create of the membership record, this will get stored into the Membership Code field so that it stays unchanged with the record over time.</description>
        <externalId>false</externalId>
        <formula>UPPER(OfferVersion__r.OfferVersionCode__c &amp; &apos;-&apos; &amp; RIGHT(Contact__c, 3) &amp; RIGHT(Id, 3))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Formula that will generate the Membership Code, then will copy to the Membership Code field.</inlineHelpText>
        <label>Membership Code Formula</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MembershipCode__c</fullName>
        <description>Membership Code unique to this Offer Version and Contact. This will not change once membership record is created.  Set via formula field and workflow.  This value can manually be overwritten as needed.</description>
        <externalId>false</externalId>
        <inlineHelpText>Membership Code unique to this Offer Version and Contact. This will not change once membership record is created.</inlineHelpText>
        <label>Membership Code</label>
        <length>16</length>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
