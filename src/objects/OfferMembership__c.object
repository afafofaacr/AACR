    <validationRules>
        <fullName>Used_Fields_Required</fullName>
        <active>false</active>
        <description>All Used fields need to be populated together.</description>
        <errorConditionFormula>(Used__c &amp;&amp; (ISBLANK(UsedDate__c) || ISBLANK(TEXT(UsedMode__c))))
||
(NOT(ISBLANK(UsedDate__c)) &amp;&amp; (NOT(Used__c) || ISBLANK(TEXT(UsedMode__c))))
||
(NOT(ISBLANK(TEXT(UsedMode__c))) &amp;&amp; (NOT(Used__c) || ISBLANK(UsedDate__c)))</errorConditionFormula>
        <errorMessage>Please fill in Used, Used Date and Used Mode together, they are all required if one is filled in.</errorMessage>
    </validationRules>
